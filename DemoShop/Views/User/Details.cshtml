@model UserDTO

<table class="table">
    <thead>
        <tr>
            <th scope="col">UserName</th>
            <th scope="col">First</th>
            <th scope="col">Last</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th scope="row">@Model.UserName</th>
            <td>@Model.FirstName</td>
            <td>@Model.LastName</td>
        </tr>
    </tbody>
</table>